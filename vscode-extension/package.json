{
  "name": "shanks-django",
  "displayName": "Shanks Django",
  "description": "Snippets and syntax highlighting for Shanks Django framework",
  "version": "0.1.3",
  "publisher": "Ararya",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Snippets",
    "Programming Languages"
  ],
  "keywords": [
    "shanks",
    "django",
    "python",
    "express",
    "api",
    "rest"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "python",
        "path": "./snippets/shanks.json"
      }
    ],
    "languages": [
      {
        "id": "python",
        "extensions": [".py"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "python",
        "scopeName": "source.python.shanks",
        "path": "./syntaxes/shanks.tmLanguage.json",
        "injectTo": ["source.python"]
      }
    ],
    "themes": [
      {
        "label": "Shanks Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/shanks-dark.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ararya/shanks-django"
  },
  "bugs": {
    "url": "https://github.com/Ararya/shanks-django/issues"
  },
  "homepage": "https://github.com/Ararya/shanks-django#readme",
  "license": "MIT"
}

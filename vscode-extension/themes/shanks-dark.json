{
  "name": "Shanks Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#0d0d0d",
    "editor.foreground": "#e5e5e5",
    "editorLineNumber.foreground": "#4a4a4a",
    "editorLineNumber.activeForeground": "#dc2626",
    "editor.selectionBackground": "#dc262633",
    "editor.lineHighlightBackground": "#1a1a1a"
  },
  "tokenColors": [
    {
      "scope": [
        "entity.name.namespace.shanks",
        "support.class.shanks",
        "support.class.sorm"
      ],
      "settings": {
        "foreground": "#dc2626",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name.function.decorator.shanks",
        "entity.name.function.sorm",
        "entity.name.function.shanks"
      ],
      "settings": {
        "foreground": "#ef4444",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "support.type.sorm.field",
        "support.type.sorm.relation"
      ],
      "settings": {
        "foreground": "#f87171"
      }
    },
    {
      "scope": [
        "constant.language.sorm"
      ],
      "settings": {
        "foreground": "#fca5a5",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name.function.sorm.query",
        "entity.name.function.shanks.response",
        "entity.name.function.shanks.app"
      ],
      "settings": {
        "foreground": "#fb923c"
      }
    },
    {
      "scope": [
        "variable.other.shanks"
      ],
      "settings": {
        "foreground": "#dc2626"
      }
    }
  ]
}
